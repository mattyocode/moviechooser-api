{
  "configurations": [
    {
      "name": "Django Shell",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/manage.py",
      "args": [
          "shell"
      ],
    },
    {
      "name": "Python Debugger: Django",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}\\manage.py",
      "args": ["runserver", "0:8000"],
      "env": {
        "DEBUG_SQL": "1",
      },
      "django": true,
      "justMyCode": true
    }
  ]
}